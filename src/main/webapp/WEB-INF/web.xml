<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description>处理用户注册</description>
  	<servlet-class>gzhu.edu.cn.homework.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SchoolServlet</servlet-name>
  	<display-name>SchoolServlet</display-name>
  	<description></description>
  	<servlet-class>gzhu.edu.cn.homework.admin.servlet.SchoolServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SchoolServlet</servlet-name>
  	<url-pattern>/admin/school</url-pattern>
  </servlet-mapping>
</web-app>
